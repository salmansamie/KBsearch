                        <TextField fx:id="keywordBox" layoutX="7.0" layoutY="66.0" prefHeight="27.0" prefWidth="145.0"
                                   promptText="Keyword"/>
                        <TextField fx:id="queryBox" layoutX="8.0" layoutY="124.0" prefHeight="27.0" prefWidth="144.0"
                                   promptText="Query"/>
                    </children>
                </AnchorPane>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                    <children>
                        <TableView fx:id="table" layoutX="185.0" layoutY="109.0" prefHeight="557.0" prefWidth="411.0"
