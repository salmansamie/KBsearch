            <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                    <children>
                        <TextField fx:id="engineBox" layoutX="7.0" layoutY="14.0" prefHeight="27.0" prefWidth="145.0"
