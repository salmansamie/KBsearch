                                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <columns>
                                <TableColumn fx:id="searchenginecol" prefWidth="136.0" text="Search Engines"/>
                                <TableColumn fx:id="keywordcol" minWidth="0.0" prefWidth="0.0" text="Keywords"/>
                                <TableColumn fx:id="querycol" prefWidth="544.0" text="Queries"/>
                            </columns>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                            </columnResizePolicy>
                        </TableView>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </center>
