                <Button fx:id="editKeyword" alignment="CENTER" mnemonicParsing="false"
                        style="-fx-background-radius: 14;" text="Edit Keyword"/>
                <Button fx:id="deleteKeyword" alignment="CENTER" mnemonicParsing="false"
                        style="-fx-background-radius: 14;" text="Remove Engine"/>
                <Pane prefHeight="29.0" prefWidth="219.0"/>
